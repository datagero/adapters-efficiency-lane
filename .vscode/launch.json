    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/cs_7643_efficiencylane"
            },
            "justMyCode": false
        },
        {
            "name": "Debug Classifier Head Training",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/cs_7643_efficiencylane/demo/finetuning/classifier_head.py",
            "args": ["roberta-base"],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/cs_7643_efficiencylane"
            },
            "justMyCode": false
        },
        {
            "name": "Debug Adapter Training",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/cs_7643_efficiencylane/demo/adapters/new_adapter.py",
            "args": ["roberta-base"],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/cs_7643_efficiencylane"
            },
            "justMyCode": false
        }  

    ]
}